{
	"schemaVersion": 1,
	"id": "acss-server-companion",
	"version": "${version}",
	"name": "ACSS Server Companion",
	"description": "Mod for running the Aston Computer Science Society's Minecraft server(s).",
	"authors": [
		"rederick29",
		"Aston Computer Science Society",
		"Git Contributors"
	],
	"contact": {
		"homepage": "https://astoncss.com/",
		"sources": "https://github.com/AstonComputerScienceSociety/acss-server-companion"
	},
	"license": "MIT or Apache-2.0",
	"icon": "assets/acss-server-companion/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.astoncss.ACSSServerCompanion"
		],
		"client": [
			"com.astoncss.ACSSServerCompanionClient"
		]
	},
	"mixins": [
		"acss-server-companion.mixins.json",
		{
			"config": "acss-server-companion.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.5",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"floodgate": ">=2.2.3-SNAPSHOT",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}
